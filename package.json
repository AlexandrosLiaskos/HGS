{
  "name": "hellenic-geo-society",
  "version": "1.0.0",
  "description": "Hellenic Geographical Society Website",
  "type": "module",
  "scripts": {
    "docs:dev": "npx vitepress dev docs",
    "docs:preview": "npx vitepress preview docs",
    "docs:serve": "npx vitepress serve docs",
    "docs:build": "npx vitepress build docs",
    "docs:deploy": "npx vitepress build docs && cd docs/.vitepress/dist && git init && git add -A && git commit -m 'deploy' && git push -f https://github.com/HGS-web/HGS.git main:main"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitepress": "^1.5.0",
    "vue": "^3.4.19"
  }
}
